<?xml version="1.0" encoding="utf-8"?>
<com.wechseltech.chartlibrary.LineChart
    xmlns:android="http://schemas.android.com/apk/res/android"
     xmlns:app="http://schemas.android.com/apk/res-auto"
     android:id="@+id/Chart"
     android:layout_width="match_parent"
     android:layout_height="match_parent"
     app:isHorizontal="false"
     app:axisLabelSize ="18sp"
     app:labelSeparation="18dp"
     app:showGrid="true"
     app:textSize="14sp"
     app:textColor="#000000"
     app:boundingboxColor="#000000"
     app:assignSeriesColors="true"
     app:gridColor="#000000"
     app:YisCategory="true"
     app:XisCategory="true"
     app:drawLabels="false"
    
    />

